	void N()
	{
		int o;
	    cout<<"are you from LPU"<<endl;
	    cout<<"1.yes or no"<<endl;
	    cin>>o;
		switch(o)
		case 1:
		{
		   int l;
		   siva:
		   	cout<<"choose your course in graduation correctly"<<endl;
		   cout<<"1.M.TECH(cse||ECE)"<<endl<<"2.Ph.D(cse||MEC||ECE||EE||CE||CHE||MOLecular biology)"<<endl;
		   cin>>l;
		    switch(l)
		    {
			case 1:
			{
				
		    int n;
			ram:
			cout<<"1.lpu test is written"<<endl<<"2.lpu test  is not written"<<endl;
			cin>>n;
			switch(n)
			{
				case 1:
					{
						float g,u;
						
						
						hug:
							char c[100]="M.tech";
							ofstream outFile;
							outFile.open("mahesh.txt",ios::app);
							outFile<<endl<<"STUDENT INFORMATION IS   : ";
					    	cout<<"enter gate marks must be in percentage form : ";
						    cin>>g;
						    outFile<<endl<<"student gate marks are :";
							outFile<<g;
							cout<<"enter lpunest marks must be in percentage form : ";
						    cin>>u;
			                outFile<<endl<<"student LPU NEST marks are :";
		    			    outFile<<u;
			                outFile<<endl<<"student course is :";
						    outFile<<c;
				            outFile.close();
						if((g>=90.0)&&(g<=100.0)||(u>=90.0)&&(u<=100))
						{
							int fees=80000;
							name();
							cout<<"student fees is:80,000"<<endl<<"student fees per sem with scholarship is :30,000";
					      	ofstream outFile;
						    outFile.open("mahesh.txt",ios::app);
						    outFile<<endl<<"student fees per sem is : ";
					        outFile<<fees;
					        outFile<<endl<<"student fees per sem with scholarship is : 30000 "; 
					        outFile.close();
						cout<<"you want to know about other course : "<<endl;
						cout<<"press Y "<<endl;
    		            cin>>ch;
    		             	if(ch=='Y'||ch=='y')
    		             	{
    		             		menu();
							 }
							 else
							 {
							 	exit(0);
							 }
						}
						else if((g>=80.0)&&(g<90)||(u>80.0)&&(u<90.0))
						
						{
